{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/events-management-app/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "src/app/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NG_APP_API_URL": "https://fsdallback.onrender.com"
  }
}
