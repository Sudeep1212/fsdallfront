{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/angular",
      "config": {
        "server": "dist/events-management-app/server/main.js"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "src/app/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NG_APP_API_URL": "https://fsdallback.onrender.com"
  }
}
