{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/angular",
      "config": {
        "server": "dist/events-management-app/server/main.js",
        "prerender": {
          "expiration": 60,
          "group": 1
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/event-registration/(?<id>[^/]+)$",
      "dest": "/event-registration/[id]"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NG_APP_API_URL": "https://fsdallback.onrender.com"
  }
}
