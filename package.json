{
  "name": "events-management-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:static": "ng build --configuration production",
    "build:ssr": "ng build --configuration production && ng run events-management-app:server:production",
    "serve:ssr": "node dist/events-management-app/server/main.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "node ./node_modules/@angular/compiler-cli/ngcc/main-ngcc.js"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-server": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@angular/ssr": "^17.2.0",
    "@types/jspdf": "^1.3.3",
    "animejs": "^4.1.3",
    "bootstrap": "^5.3.8",
    "express": "^5.1.0",
    "jspdf": "^3.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@angular/cli": "^17.2.0",
    "@angular/compiler-cli": "^17.2.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": "20.x",
    "npm": "9.x"
  },
  "installConfig": {
    "legacy-peer-deps": true
  }
}
